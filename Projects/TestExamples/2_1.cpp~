# include <iostream>
using namespace std;

int main1(){
	unsigned int x;
	unsigned int y = 100;
	unsigned int z = 50; 
	
	x = y - z;
	cout<<"Difference is:"<<x;
	x = z - y;
    // 由于无符号整数没有负数，这里强行转换成无符号整数，结果看着很大 
	cout<<"\n Now difference is:"<<x<<endl; 
	cin>>x;
	cout<<"x:"<<x<<endl;
	return 0;
	
} 